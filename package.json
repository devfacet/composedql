{
  "name": "composedql",
  "version": "0.7.1",
  "description": "Composed query language parser",
  "keywords": [
    "composed",
    "query",
    "language",
    "json",
    "object",
    "rest",
    "resource",
    "uri",
    "url"
  ],
  "homepage": "http://github.com/fatihcode/composedql",
  "repository": {
    "type": "git",
    "url": "https://github.com/fatihcode/composedql.git"
  },
  "bugs": {
    "url": "http://github.com/fatihcode/composedql/issues"
  },
  "license": "MIT",
  "private": false,
  "author": {
    "name": "fatih",
    "url": "http://github.com/fatihcode"
  },
  "contributors": [],
  "main": "./index.js",
  "scripts": {
    "lint": "jshint --reporter node_modules/jshint-stylish lib/* test/*",
    "lint:build": "jshint --reporter checkstyle lib/* test/* > reports/jshint-checkstyle.xml",
    "depcheck": "dependency-check . lib/*",
    "depcheck:unused": "dependency-check ./package.json --unused --no-dev lib/*",
    "test:unit": "mkdir -p reports/ && NODE_ENV=test multi='spec=- xunit=reports/mocha-xunit.xml' istanbul cover _mocha -- -R mocha-multi --timeout 10000 && istanbul check-coverage",
    "test": "npm run depcheck && npm run depcheck:unused && npm run lint && npm run test:unit"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "3.2.x",
    "jshint": "2.8.x",
    "jshint-stylish": "2.0.x",
    "mocha": "2.2.x",
    "mocha-multi": "0.7.x",
    "dependency-check": "2.5.x",
    "istanbul": "^0.3.17"
  }
}